{
  "name": "finance-management-system",
  "version": "1.0.0",
  "description": "Sistema completo de gerenciamento financeiro com backend em Node.js e frontend em Next.js",
  "scripts": {
    "setup": "node setup.js",
    "dev:backend": "cd server && npm run dev",
    "dev:frontend": "cd web && npm run dev",
    "install:all": "npm install && cd server && npm install && cd ../web && npm install",
    "seed": "cd server && npm run seed",
    "db:studio": "cd server && npm run db:studio",
    "db:reset": "cd server && npm run db:reset"
  },
  "keywords": [
    "finance",
    "management",
    "nodejs",
    "typescript",
    "fastify",
    "prisma",
    "nextjs",
    "react"
  ],
  "author": "Gabriel Ribeiro Vitor",
